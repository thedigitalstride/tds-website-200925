    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "4.5.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "gray-matter": "^4.0.3",
    "jsdom": "26.1.0",
    "mongodb": "^7.0.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
