    "@payloadcms/plugin-search": "3.61.1",
    "@payloadcms/plugin-seo": "3.61.1",
    "@payloadcms/richtext-lexical": "3.61.1",
    "@payloadcms/storage-vercel-blob": "3.61.1",
    "@payloadcms/ui": "3.61.1",
