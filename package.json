{
  "name": "with-vercel-website",
  "version": "1.0.0",
  "description": "The Digital Stride - More Leads. Better Customers. Faster Growth.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "preinstall": "node scripts/manage-motion-token.js inject",
    "postinstall": "node scripts/manage-motion-token.js restore",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "ci": "payload migrate && pnpm build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:stable": "./scripts/dev-stable.sh",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "node ./scripts/check-migrations.js",
    "payload:unsafe": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "migrate:create": "node ./scripts/check-migrations.js migrate:create",
    "dev:check": "echo 'âœ… Using dev server for auto-sync (no migrations needed)' && pnpm dev",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "fix:uui": "node ./scripts/fix-uui-structure.js",
    "uui:add": "npx untitledui@latest add $1 --path src/components/uui && pnpm fix:uui",
    "seed:posts": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/seed-posts.ts"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.59.1",
    "@payloadcms/db-vercel-postgres": "3.59.1",
    "@payloadcms/drizzle": "^3.59.1",
    "@payloadcms/email-resend": "^3.59.1",
    "@payloadcms/live-preview-react": "3.59.1",
    "@payloadcms/next": "3.59.1",
    "@payloadcms/payload-cloud": "3.59.1",
    "@payloadcms/plugin-form-builder": "3.59.1",
    "@payloadcms/plugin-nested-docs": "3.59.1",
    "@payloadcms/plugin-redirects": "3.59.1",
    "@payloadcms/plugin-search": "3.59.1",
    "@payloadcms/plugin-seo": "3.59.1",
    "@payloadcms/richtext-lexical": "3.59.1",
    "@payloadcms/storage-vercel-blob": "3.59.1",
    "@payloadcms/ui": "3.59.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-stately/utils": "^3.10.8",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "0.5.0-alpha.3",
    "@untitledui/icons": "^0.0.19",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "hamburger-react": "^2.5.2",
    "lucide-react": "^0.378.0",
    "motion": "^12.23.22",
    "motion-plus": "https://api.motion.dev/registry?package=motion-plus&version=1.5.4&token=293989864085e085c364fe33dcc4c31064d8dae1856dde648e217e291e062241",
    "next": "15.4.4",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "payload": "3.59.1",
    "postcss": "^8.5.6",
    "prism-react-renderer": "^2.3.1",
    "psql": "^0.0.1",
    "react": "19.1.0",
    "react-aria": "^3.42.0",
    "react-aria-components": "^1.12.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.45.4",
    "react-stately": "^3.41.0",
    "sharp": "0.34.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "1.54.1",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "prettier": "^3.4.2",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
