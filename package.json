{
  "name": "with-vercel-website",
  "version": "1.0.0",
  "description": "The Digital Stride - More Leads. Better Customers. Faster Growth.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "ci": "pnpm build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:watch": "pnpm run lint:watch & pnpm dev",
    "dev:stable": "./scripts/dev-stable.sh",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS='--no-deprecation --import ./preload-css-handler.mjs' payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "lint:watch": "cross-env NODE_OPTIONS=--no-deprecation next lint --watch",
    "lint:watch:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --watch --fix",
    "payload": "node ./scripts/check-migrations.js",
    "payload:unsafe": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "migrate:create": "node ./scripts/check-migrations.js migrate:create",
    "dev:check": "echo 'âœ… Using dev server for auto-sync (no migrations needed)' && pnpm dev",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "fix:uui": "node ./scripts/fix-uui-structure.js",
    "uui:add": "npx untitledui@latest add $1 --path src/components/uui && pnpm fix:uui",
    "seed:posts": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/seed-posts.ts",
    "preflight": "node scripts/preflight-check.js",
    "mongodb:pull": "node scripts/migrate-mongodb-to-local.js"
  },
  "dependencies": {
